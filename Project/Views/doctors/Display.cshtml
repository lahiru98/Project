@model IEnumerable<Project.Models.doctor>

@{

    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <center><h2>Doctor Records</h2></center>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.did)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dage)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dgender)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.daddress)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dcategory)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.password)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.confirmpassword)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.status)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.did)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.dname)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.dage)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.dgender)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.daddress)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.dcategory)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.password)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.confirmpassword)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.status)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.did }) |
                    @Html.ActionLink("Details", "Details", new { id = item.did }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.did })
                </td>
            </tr>
        }

    </table>
</body>
</html>
